{
    "java.project.referencedLibraries": [
        "lib/mysql-connector-j-9.2.0.jar"  // Path to your JAR
    ],
    "java.project.outputPath": "bin",
    "sqltools.connections": [
        {
            "mysqlOptions": {
                "authProtocol": "default",
                "enableSsl": "Disabled"
